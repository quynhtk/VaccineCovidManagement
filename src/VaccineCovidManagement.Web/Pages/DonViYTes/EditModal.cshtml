@page
@model VaccineCovidManagement.Web.Pages.DonViYTes.EditModalModel
@using Microsoft.Extensions.Localization
@using VaccineCovidManagement.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@inject IStringLocalizer<VaccineCovidManagementResource> L
@{
    Layout = null;
}

<abp-card>
    <form method="post" asp-page="/DonViYTes/EditModal">
        <abp-modal>
            <abp-modal-header></abp-modal-header>
            <abp-modal-body>
                <abp-input asp-for="EditDonViYTes.Id"></abp-input>
                <abp-input asp-for="EditDonViYTes.TenDonViYTe"></abp-input>
                <abp-input asp-for="EditDonViYTes.DiaChi"></abp-input>
                <abp-input asp-for="EditDonViYTes.SDT"></abp-input>
            </abp-modal-body>
            <abp-modal-footer buttons="@(AbpModalButtons.Cancel| AbpModalButtons.Save)"></abp-modal-footer>
        </abp-modal>
    </form>
</abp-card>
