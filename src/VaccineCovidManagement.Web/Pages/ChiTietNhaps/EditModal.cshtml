@page
@model VaccineCovidManagement.Web.Pages.ChiTietNhaps.EditModalModel
@using Microsoft.Extensions.Localization
@using VaccineCovidManagement.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@inject IStringLocalizer<VaccineCovidManagementResource> L
@{
    Layout = null;
}

<abp-card>
    <form method="post" asp-page="/ChiTietNhaps/EditModal">
        <abp-modal>
            <abp-modal-header></abp-modal-header>
            <abp-modal-body>
                <abp-input asp-for="EditChiTietNhaps.Id"></abp-input>
                <abp-select asp-for="EditChiTietNhaps.NhaSxID" asp-items="@Model.NhaSanXuats"></abp-select>
                @*<abp-input asp-for="EditChiTietNhaps.TenVaccineSX" disabled></abp-input>*@
                <abp-input asp-for="EditChiTietNhaps.NgaySx"></abp-input>
                <abp-input asp-for="EditChiTietNhaps.HanSuDung"></abp-input>
                <abp-input asp-for="EditChiTietNhaps.SoLuongNhap"></abp-input>
            </abp-modal-body>
            <abp-modal-footer buttons="@(AbpModalButtons.Cancel| AbpModalButtons.Save)"></abp-modal-footer>
        </abp-modal>
    </form>
</abp-card>
