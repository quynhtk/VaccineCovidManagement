@page
@model VaccineCovidManagement.Web.Pages.ChiTietXuats.CreateModalModel
@using Microsoft.Extensions.Localization
@using VaccineCovidManagement.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@inject IStringLocalizer<VaccineCovidManagementResource> L
@{
    Layout = null;
}
<abp-card>
    <form method="post" asp-page="/ChiTietXuats/CreateModal">
        <abp-modal>
            <abp-modal-header></abp-modal-header>
            <abp-modal-body>
                <abp-select asp-for="CreateChiTietXuat.DonViID" asp-items="@Model.DonViYTes"/>
                <abp-select asp-for="CreateChiTietXuat.VaccineTonKhoID" asp-items="@Model.Vaccines"/>
                <abp-input asp-for="CreateChiTietXuat.SoLuongXuat"/>
            </abp-modal-body>
            <abp-modal-footer buttons="@(AbpModalButtons.Cancel| AbpModalButtons.Save)">
            </abp-modal-footer>
        </abp-modal>
    </form>
</abp-card>