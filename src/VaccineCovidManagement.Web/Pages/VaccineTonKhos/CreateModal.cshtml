@page
@model VaccineCovidManagement.Web.Pages.VaccineTonKhos.CreateModalModel
@using Microsoft.Extensions.Localization
@using VaccineCovidManagement.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@inject IStringLocalizer<VaccineCovidManagementResource> L
@{
    Layout = null;
}

<abp-card>
    <form method="post" asp-page="/VaccineTonKhos/CreateModal">
        <abp-modal>
            <abp-modal-header></abp-modal-header>
            <abp-modal-body>
                <abp-select asp-for="CreateVaccineTonKhos.TenVaccineTonKho" asp-items="@Model.TenVaccine" ></abp-select>
                <abp-input asp-for="CreateVaccineTonKhos.SoLuongTonKho"></abp-input>
            </abp-modal-body>
            <abp-modal-footer buttons="@(AbpModalButtons.Cancel| AbpModalButtons.Save)"></abp-modal-footer>
        </abp-modal>
    </form>
</abp-card>